{
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "extends": ["eslint:recommended", "plugin:import/errors"],
  "settings": {
  },
  "rules": {
    "max-len": [1, {"code": 150}],
    "no-underscore-dangle": [0],
    "new-cap": [0],
    "arrow-parens": [0],
    "consistent-return": [0],
    "no-confusing-arrow": [0],
    "import/prefer-default-export": [0],
    "import/no-named-as-default": [0],
  }
}
